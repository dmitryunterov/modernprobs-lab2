#include <tunables/global>

/usr/bin/lftp {
  #include <abstractions/base>
  #include <abstractions/nameservice>
  /etc/lftp.conf r,
  /usr/bin/lftp mr,
  @{HOME}/ rw,
  /home/*/.local/share/lftp/* rw,  
  /home/*/* rw,
  /etc/inputrc rw,
  /etc/nsswitch.conf rw,
  /etc/host.conf rw,
  /etc/resolv.conf rw,
  /etc/hosts rw,
}
